@model MugOrder



<div class="form-group row">
    <label asp-for="Email" class="col-3 control-label">Email</label>
    <div class="col-7">
        <input id="txtQty" asp-for="Email" class="form-control" />
    </div>
</div>

<div class="form-group row">
    <label asp-for="FullName" class="col-3 control-label">Full Name</label>
    <div class="col-7">
        <input id="txtQty" asp-for="FullName" class="form-control" />
    </div>
</div>

<div class="form-group row">
    <label asp-for="Courses" class="col-3 control-label">Courses I can conduct</label>

    <div class="col-7">
        <input id="txtQty" asp-for="Courses" class="form-control" />
    </div>
</div>


<div class="form-group row">
    <label asp-for="AdditionalCourses" class="col-3 control-label">Additional Courses</label>
    <div class="col-7" id="txtQty1">
        <input type="text" asp-for="AdditionalCourses" id="txtQty" class="form-control" />
    </div>
</div>


<div class="form-group row">
    <label asp-for="DOB" class="col-3 control-label">DOB</label>
    <div class="col-7">
        <input id="txtQty" asp-for="DOB" class="form-control" />
    </div>
</div>

